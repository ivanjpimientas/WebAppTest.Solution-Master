@model WebAppTest.Core.Entities.Editorial
<div>
    <div class="row">
        <div class="col-md-12">
            <form id="editorialForm">
                <input type="hidden" asp-for="Id" />
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="Email" class="control-label"></label>
                    <input asp-for="Email" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="AddressEditorial" class="control-label"></label>
                    <input asp-for="AddressEditorial" class="form-control" />
                </div>
                <div class="form-group">
                    <label asp-for="CountBooks" class="control-label"></label>
                    <input asp-for="CountBooks" class="form-control" />
                </div>
                <div class="form-group">
                    <button type="button" id="btnSubmit" onclick="webapptest.editorials_manager.saveEditorialForm()" class="btn btn-block btn-info">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>